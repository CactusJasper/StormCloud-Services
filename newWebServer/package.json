{
  "name": "new-web-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "ts-node-dev ./src/main.ts",
    "prebuild": "npm i && rimraf dist",
    "build": "npx @nestjs/cli build",
    "start": "npm run prebuild && npm run build && npm run build:scss && node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "scss": "sass --watch ./scss/main.scss ./public/css/main.css",
    "build:scss": "sass ./scss/main.scss ./public/css/main.css"
  },
  "dependencies": {
    "@discord-nestjs/common": "^2.0.17",
    "@discord-nestjs/core": "^3.0.1",
    "@handlebars/allow-prototype-access": "^1.0.5",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/schedule": "^1.0.2",
    "axios": "^0.26.1",
    "body-parser": "^1.19.2",
    "class-validator": "^0.13.2",
    "discord.js": "^13.6.0",
    "express-handlebars": "^5.3.4",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.9.8",
    "hbs": "^4.1.2",
    "moment": "^2.29.1",
    "mongoose": "^6.0.10",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "passport-oauth2": "^1.6.1",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/passport-discord": "^0.1.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
